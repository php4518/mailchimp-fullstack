{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "no-shadow": "off",
    "no-useless-escape": "off",
    "react/jsx-filename-extension": "off",
    "react/prefer-stateless-function": "off",
    "react/no-unused-state": "off",
    "react/prop-types": "off"
  }
}
